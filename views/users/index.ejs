<h2>Users Table</h2>
<table class="table">
    <thread>
        <tr>
            <th>UserID</th>
            <th>Name</th>
            <th>Email</th>
            <th>Password</th>
        </tr>
    </thread>
    <tbody>
        <% users.forEach(user => 
        { %>
            <tr>
                <td><%= user.userid %></td>
                <td>
                    <a href="<%= `/users/${user._id}` %>">
                        <%= user.fullName %>
                    </a>
                </td>
                <td><%= user.email %></td>
                <td><%= user.password %></td>
            </tr>
        <% }); %>
    </tbody>
</table>